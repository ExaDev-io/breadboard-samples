{
	"dependencies": {
		"@exadev/breadboard-kits": "^0.9.1",
		"@google-labs/breadboard": "^0.6.0",
		"@google-labs/core-kit": "0.1.0",
		"@google-labs/llm-starter": "0.3.0",
		"@paulkinlan/claude-breadboard-kit": "^2.2.5"
	},
	"devDependencies": {
		"@types/node": "^20.10.7",
		"typescript": "^5.3.3",
		"wireit": "^0.14.1"
	},
	"engines": {
		"node": ">=19"
	},
	"engineStrict": true,
	"name": "coursecrafter",
	"private": true,
	"scripts": {
		"build": "wireit",
		"run-board": "wireit",
		"run-board-multiple": "wireit",
		"test": "wireit",
		"validate": "wireit",
		"watch-board": "wireit",
		"watch-board-multiple": "wireit"
	},
	"type": "module",
	"version": "0.0.0",
	"wireit": {
		"build": {
			"command": "bash -c 'set -o pipefail; tsc --noEmit --listFiles | grep -v \"node_modules\"'"
		},
		"run-board": {
			"command": "npx -y tsx src/single/index.ts"
		},
		"run-board-multiple": {
			"command": "npx -y tsx src/multiple/index.ts"
		},
		"test": {
			"dependencies": [
				"run-board",
				"run-board-multiple"
			]
		},
		"validate": {
			"dependencies": [
				"build",
				"test"
			]
		},
		"watch-board": {
			"command": "npx -y tsx watch src/single/index.ts"
		},
		"watch-board-multiple": {
			"command": "npx -y tsx watch src/multiple/index.ts"
		}
	}
}
