{
  "title": "The Panel",
  "description": "A Panel of Critics",
  "version": "0.0.1",
  "edges": [
    {
      "from": "input-text",
      "to": "critic1-lambda",
      "out": "article",
      "in": "article"
    },
    {
      "from": "critic1-lambda",
      "to": "invoke-2",
      "constant": true,
      "out": "board",
      "in": "board"
    },
    {
      "from": "critic1-input",
      "to": "invoke-2",
      "out": "id",
      "in": "id"
    },
    {
      "from": "critic1-input",
      "to": "invoke-2",
      "out": "persona",
      "in": "persona"
    },
    {
      "from": "critic1-input",
      "to": "invoke-2",
      "out": "name",
      "in": "name"
    },
    {
      "from": "invoke-2",
      "to": "output-1",
      "out": "*"
    }
  ],
  "nodes": [
    {
      "id": "input-text",
      "type": "input"
    },
    {
      "id": "output-1",
      "type": "output"
    },
    {
      "id": "critic1-input",
      "type": "input"
    },
    {
      "id": "critic1-lambda",
      "type": "lambda",
      "configuration": {
        "board": {
          "kind": "board",
          "board": {
            "title": "The Critic",
            "description": "Test Breadboard Kit",
            "version": "0.0.1",
            "edges": [
              {
                "from": "critic-name",
                "to": "promptTemplate-3",
                "out": "persona",
                "in": "persona"
              },
              {
                "from": "critic-name",
                "to": "promptTemplate-3",
                "out": "article",
                "in": "article"
              },
              {
                "from": "critic-name",
                "to": "promptTemplate-3",
                "out": "name",
                "in": "name"
              },
              {
                "from": "promptTemplate-3",
                "to": "generateCompletion-4",
                "out": "prompt",
                "in": "text"
              },
              {
                "from": "secrets-2",
                "to": "generateCompletion-4",
                "out": "CLAUDE_API_KEY",
                "in": "CLAUDE_API_KEY"
              },
              {
                "from": "generateCompletion-4",
                "to": "output-1",
                "out": "completion",
                "in": "response"
              },
              {
                "from": "critic-name",
                "to": "output-1",
                "out": "name",
                "in": "name"
              },
              {
                "from": "critic-name",
                "to": "output-1",
                "out": "id",
                "in": "id"
              }
            ],
            "nodes": [
              {
                "id": "output-1",
                "type": "output"
              },
              {
                "id": "critic-name",
                "type": "input",
                "configuration": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "title": "Critic Name",
                        "description": "The name of the Critic"
                      },
                      "id": {
                        "type": "string",
                        "title": "id",
                        "description": "The id of the critique being created"
                      },
                      "article": {
                        "type": "string",
                        "title": "articleToCritique",
                        "description": "The article that is being critiqued"
                      },
                      "persona": {
                        "type": "string",
                        "title": "Critic Persona",
                        "description": "The Persona of the Critic"
                      }
                    }
                  }
                }
              },
              {
                "id": "secrets-2",
                "type": "secrets",
                "configuration": {
                  "keys": [
                    "CLAUDE_API_KEY"
                  ]
                }
              },
              {
                "id": "promptTemplate-3",
                "type": "promptTemplate",
                "configuration": {
                  "template": "\nYour name is {{name}} and you are a {{persona}}.\n\nYou will create a critique of the following input:\n\n{{article}}\n\nCritique:\n"
                }
              },
              {
                "id": "generateCompletion-4",
                "type": "generateCompletion",
                "configuration": {
                  "model": "claude-2.1",
                  "baseURL": "http://localhost:5173"
                }
              }
            ],
            "kits": [
              {
                "title": "LLM Starter Kit",
                "description": "A kit that provides a few necessary components for wiring boards that use PaLM API.",
                "version": "0.0.1",
                "url": "npm:@google-labs/llm-starter"
              },
              {
                "url": "npm:@paulkinlan/claude-breadboard-kit"
              }
            ]
          }
        },
        "name": "Paul",
        "persona": "The Critic"
      }
    },
    {
      "id": "invoke-2",
      "type": "invoke",
      "configuration": {}
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/core-kit"
    }
  ]
}