{
	"dependencies": {
		"@exadev/breadboard-kits": "^0.10.1",
		"@google-labs/breadboard": "^0.30.0",
		"@google-labs/core-kit": "^0.17.0",
		"@paulkinlan/claude-breadboard-kit": "^3.1.0",
		"@types/puppeteer": "^7.0.4",
		"puppeteer": "^23.10.4",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"wireit": "^0.14.1"
	},
	"name": "@exadev/breadboard-autobake",
	"private": true,
	"scripts": {
		"build": "wireit",
		"test": "wireit",
		"validate": "wireit",
		"watch": "wireit"
	},
	"type": "module",
	"version": "0.0.0",
	"wireit": {
		"build": {
			"command": "bash -c 'set -o pipefail; tsc --noEmit --listFiles | grep -v \"node_modules\"'",
			"files": [
				"src/**/*.ts",
				"tsconfig.json",
				"package.json",
				"../../tsconfig.minimal.json"
			]
		},
		"test": {
			"command": "tsx src/breadboard/autobake.ts",
			"dependencies": [
				"build"
			],
			"files": [
				"src/**/*.ts",
				"tsconfig.json",
				"package.json",
				"../../tsconfig.minimal.json"
			]
		},
		"validate": {
			"dependencies": [
				"test",
				"build"
			]
		},
		"watch": {
			"command": "tsx watch src/breadboard/runBoard.ts"
		}
	}
}
