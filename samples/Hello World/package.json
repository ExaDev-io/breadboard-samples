{
	"dependencies": {
		"@google-labs/breadboard": "^0.6.0"
	},
	"devDependencies": {
		"@types/node": "^20.9.3",
		"typescript": "^5.3.2",
		"wireit": "^0.14.1"
	},
	"engines": {
		"node": ">=19"
	},
	"engineStrict": true,
	"name": "hello-breadboard",
	"scripts": {
		"build": "wireit",
		"start": "wireit",
		"test": "wireit",
		"validate": "wireit"
	},
	"type": "module",
	"wireit": {
		"build": {
			"command": "tsc --noEmit --listFiles | grep -v 'node_modules'"
		},
		"start": {
			"command": "npx -y tsx index.ts watch"
		},
		"test": {
			"command": "npx -y tsx index.ts test"
		},
		"validate": {
			"dependencies": [
				"build",
				"test"
			]
		}
	}
}
